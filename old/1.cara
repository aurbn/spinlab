t.okbutton:setCallback( gui.event.Clicked,
	function (self)

-- ================== Set the user preference variables ==================

-- get the project
for ProjId,Proj in pairs( cara:getProjects() ) do
	if Proj:getName() == t.ProjectListCB:getCurrentText() then
