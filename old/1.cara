            else if(num_of_dim==3)then amplitude=spectrum:getAt(x,d2,d3);
            else if(num_of_dim==2)then amplitude=spectrum:getAt(x,d2);
            else if(num_of_dim==1)then amplitude=spectrum:getAt(x);
