function FoldedShift( Spectrum, Dim, Shift ) -- calculates folded position of shift
	PpmAtPt0,PpmAtPtNminus1 = Spectrum:getPpmRange( Dim );
	local FoldedShiftValue
	local SpectralWidth = math.abs(PpmAtPt0 - PpmAtPtNminus1 );
	local CarrierPpm = (PpmAtPt0 + PpmAtPtNminus1 ) / 2;
	if SpectralWidth == 0 then error("Error: 3D has SpectralWidth==0 in dim "..Dim ) end
	
	if Shift &gt; PpmAtPt0 then
		-- Shift is above maximal ppm
		DistanceInPpm = Shift - PpmAtPt0
		FoldRemainder = math.mod( DistanceInPpm, SpectralWidth )
		if Folding=="RSH" then
			FoldedShiftValue = PpmAtPtNminus1 + FoldRemainder
		elseif Folding=="TPPI" then
			FoldedShiftValue = PpmAtPt0 - FoldRemainder
		else -- same as RSH
			FoldedShiftValue = PpmAtPtNminus1 + FoldRemainder
		end
	elseif Shift &lt; PpmAtPtNminus1 then
		-- Shift is below minimal ppm
		DistanceInPpm = PpmAtPt0 - Shift
		FoldRemainder = math.mod( DistanceInPpm, SpectralWidth )
		if Folding=="RSH" then
			FoldedShiftValue = PpmAtPt0 - FoldRemainder
		elseif Folding=="TPPI" then
			FoldedShiftValue = PpmAtPtNminus1 + FoldRemainder
		else -- same as RSH
			FoldedShiftValue = PpmAtPt0 - FoldRemainder
		end
	else
		-- Shift is with SpectralWidth
		FoldedShiftValue = Shift
	end
	
	--[[ -- first try, couldn't get this to work, FFD.
	FoldRemainder = math.mod( (Shift - CarrierPpm ) / SpectralWidth + 0.5, 1 ) - 0.5;
	Folding = Spectrum:getFolding( Dim );
	if (Folding == "RSH") then
		FoldedShiftValue = CarrierPpm + FoldRemainder * SpectralWidth;
	elseif (Folding == "TPPI") then
		FoldedShiftValue = CarrierPpm - FoldRemainder * SpectralWidth;
	else-- default is RSH
		FoldedShiftValue = CarrierPpm + FoldRemainder * SpectralWidth;
	end
	if math.abs( Shift - FoldedShiftValue ) &gt; 0.05 then
		print("SW="..SpectralWidth) -- TEST TEST TEST
		print(" CarrierPpm="..CarrierPpm)
		print(" PpmAtPtO="..PpmAtPt0)
		print(" PpmAtPtNminus1="..PpmAtPtNminus1)
		print("Shift = "..Shift)
		print("FoldRemainder="..FoldRemainder)
		print("Formula FoldedShift=PpmAtPtNminus1 + FoldRemainder ")
		print("or")
		print("Formula FoldedShift=PpmAtPt0 - FoldRemainder ")
		print(" FoldedShiftValue="..FoldedShiftValue) -- TEST TEST TEST
	end
		]]

	
	return FoldedShiftValue
end -- function FoldedShift
