				  end
				  if b:getResidue() ~= nil then
					   t.P:unassignSystem(b)
				  end
				   if b:getSpins() ~= nil then
			--          print("has spins")
					  t.SpinsInSys = b:getSpins()
			--      remove Spins in System
					 for c,d in pairs(t.SpinsInSys) do
