SpinsInColumns = {}
SpinsInColumns[ 1 ] = "H"
SpinsInColumns[ 2 ] = "HA"
SpinsInColumns[ 3 ] = "N"
SpinsInColumns[ 4 ] = "CA"
SpinsInColumns[ 5 ] = "CB"

